    agent any

    stages {
        stage('build') {
            steps {
                echo 'Hello World this is build phase'
            }
        }
        stage('test') {
            steps {
                echo 'Hello World this is test phase'
            }
        }
        stage('deploy') {
            steps {
                echo 'Hello World ths is deploy phase'
            }
        }
    }
}
